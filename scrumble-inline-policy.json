{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::theatrico-site-dev",
                "arn:aws:s3:::theatrico-site-prod"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::theatrico-site-dev/*",
                "arn:aws:s3:::theatrico-site-prod/*"
            ]
        }
    ]
}
